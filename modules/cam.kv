# kv file


<WelcomeScreen>:
    name: '_wlcm_screen_'
    chooser: chooser

    GridLayout:
        cols: 2
        #rows: 10

        Label:
            text: "Practice"

        RelativeLayout:
            id: chooser
            ToggleButton:
                text: "Bicep"
                group: 'practice'
                size_hint: None,None
                size: 0.08 * root.width, 0.05 * root.height
                pos: 0.12 * root.width, 0.350 * root.height

            ToggleButton:
                text: "Push Ups"
                group: 'practice'
                size_hint: None,None
                size: 0.1 * root.width, 0.05 * root.height
                pos: 0.23 * root.width, 0.350 * root.height